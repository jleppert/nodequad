--http-client-body-temp-path=/home/www/tmp/nginx/proxy/client-temp/ --http-proxy-temp-path=/home/www/tmp/nginx/proxy/ --http-fastcgi-temp-path=/home/www/tmp/nginx/fastcgi/ --http-uwsgi-temp-path=/home/www/tmp/nginx/uwsgi/ --http-scgi-temp-path=/home/www/tmp/nginx/scgi/ --user=www --group=www